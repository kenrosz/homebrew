prefix=/opt/homebrew/Cellar/libass/0.17.1_1
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libass
Description: LibASS is an SSA/ASS subtitles rendering library
Version: 0.17.1
Requires: 
Requires.private: libunibreak >= 1.1, harfbuzz >= 1.2.3, fribidi >= 0.19.1, freetype2 >= 9.17.3
Libs: -L${libdir} -lass 
Libs.private: -liconv  -framework ApplicationServices -framework CoreFoundation
Cflags: -I${includedir}
